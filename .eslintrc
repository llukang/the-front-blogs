{
  "parser": "babel-eslint",
  "extends": "eslint-config-airbnb",
  "rules": {
    "react/destructuring-assignment": 0,
    "object-curly-newline": 0,
    "jsx-a11y/click-events-have-key-events": 0,
    "jsx-a11y/no-static-element-interactions": 0,
    "indent": [
      1,
      2,
      {
        "SwitchCase": 1
      }
    ],
    "no-unused-expressions": [
      1,
      {
        "allowShortCircuit": true,
        "allowTernary": true
      }
    ],
    "destructuring": 0,
    "max-len": [
      0,
      100
    ],
    "import/no-unresolved": 0,
    "global-require": 0,
    "new-cap": 0,
    "comma-dangle": 0,
    "no-case-declarations": 0,
    "eqeqeq": 0,
    "no-confusing-arrow": 0,
    "arrow-body-style": 0,
    "react/prop-types": 0,
    "no-param-reassign": 0,
    "react/jsx-indent": [
      1,
      2
    ],
    "react/jsx-indent-props": [
      1,
      2
    ],
    "react/jsx-no-bind": 0,
    "linebreak-style":0,
    "react/prefer-stateless-function": 0,
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [
          ".js",
          ".jsx"
        ]
      }
    ]
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true
    }
  },
  "globals": {
    "document": false,
    "Prism": false,
    "fetch": false,
    "React": false,
    "window": false,
    "editormd": false,
    "describe": false,
    "it": false,
    "expect": false
  }
}
